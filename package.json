{
	"name": "@clearmylist/routes",
	"version": "0.8.9",
	"description": "API routes of https://ClearMyList.io.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"private": false,
	"scripts": {
		"lint": "eslint . --fix --max-warnings=0",
		"format": "prettier -w .",
		"test": "",
		"postinstall": "husky install",
		"prepack": "pinst --disable",
		"postpack": "pinst --enable",
		"tsc": "tsc --build --force",
		"pub": "rm -rf dist; npm run tsc && npm publish --access public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ClearMyList/routes.git"
	},
	"keywords": [
		"clearmylist",
		"routes"
	],
	"author": {
		"name": "Clear My List",
		"url": "https://clearmylist.io"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ClearMyList/routes/issues"
	},
	"homepage": "https://github.com/ClearMyList/routes#readme",
	"husky": {
		"hooks": {
			"pre-commit": "yarn lint"
		}
	},
	"dependencies": {
		"eslint": "8.38.0",
		"eslint-config-airbnb-typescript": "17.0.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-import": "2.27.5"
	},
	"devDependencies": {
		"@types/node": "18.15.11",
		"husky": "8.0.3",
		"pinst": "3.0.0",
		"tslint": "6.1.3",
		"typescript": "5.0.4"
	}
}
